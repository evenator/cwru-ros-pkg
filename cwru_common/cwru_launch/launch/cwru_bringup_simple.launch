<launch>
	<!-- Startup the base and sensors -->
	<include file="$(find cwru_base)/start_base_simple.launch" />

	<!-- Startup the joystick stuff -->
	<include file="$(find cwru_teleop)/start_teleop.launch" />
</launch>
